{
  "name": "@heyooo-inc/utils",
  "version": "0.0.6",
  "description": "A JavaScript utility library",
  "author": "Heyooo-Inc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heyooo-Inc/utils.git"
  },
  "bugs": {
    "url": "https://github.com/Heyooo-Inc/utils/issues"
  },
  "homepage": "https://github.com/Heyooo-Inc/utils#readme",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest --env=jsdom",
    "cov": "jest --env=jsdom --coverage",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write ./src",
    "lint": "eslint . --ext .ts --quiet",
    "prebuild": "rimraf lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "peerDependencies": {
    "deepmerge": "^4.3.1",
    "rfdc": "^1.3.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "deepmerge": "^4.3.1",
    "rfdc": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "coveralls": "^3.1.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}